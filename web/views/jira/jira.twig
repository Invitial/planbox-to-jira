{% extends "layout.html" %}

{% block content %}
<div class="container">
    <div class="page-header">
        <h1>Your JIRA Projects</h1>
    </div>
    <h3>Select the JIRA Project To Import Into</h3>
    <div class="list-group">
        {% for item in jiraProjects %}
          <a class="list-group-item" href="/jira/{{ item.id }}">[{{ item.key }}] - {{ item.name }}</a>
        {% endfor %}
    </div>
</div>
{% endblock %}
